#!/usr/bin/env bash

# Kill child processes on ^C
trap "exit" INT TERM ERR
trap "kill 0" EXIT

# Run loops in parallel in background
m3d_request_loop &
mcb_request_loop &
mfx_request_loop &
mms_maintenance_loop &
mms_worker_loop &

wait

echo "#####################################"
echo "M3D dev loops finished or errored out"
echo "#####################################"
